docker run --name elasticsearch -e ELASTIC_PASSWORD=elasticsearch -e xpack.security.enabled='false' -e xpack.security.transport.ssl.enabled='false' -p 9200:9200 -d docker.elastic.co/elasticsearch/elasticsearch:8.12.1
docker run --name kibana -e ELASTICSEARCH_HOSTS=http://localhost:9200 -e ELASTICSEARCH_PASSWORD=elasticsearch -e ELASTICSEARCH_USERNAME=kibana_system -e SERVERNAME=kibana -p 5601:5601 -d docker.elastic.co/kibana/kibana:8.12.1
